{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 80,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "jsxSingleQuote": true,
  "quoteProps": "consistent",
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.ts, *tsx",
      "options": {
        "printWidth": 80
      }
    }
  ]
}
